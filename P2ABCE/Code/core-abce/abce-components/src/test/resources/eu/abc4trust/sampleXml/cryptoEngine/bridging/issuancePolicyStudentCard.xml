<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- This is a sample ABC4Trust Issuance Policy for a student card. -->

<abc:IssuancePolicy Version="1.0" xmlns:abc="http://abc4trust.eu/wp2/abcschemav1.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://abc4trust.eu/wp2/abcschemav1.0 ../../../../../../../../../abc4trust-xml/src/main/resources/xsd/schema.xsd">
  <abc:PresentationPolicy 
  	PolicyUID="http://www.admin.edu/studentid/issuance/policy"/>
  <abc:CredentialTemplate>
    <abc:CredentialSpecUID>urn:patras:studentcredspec</abc:CredentialSpecUID>
    <abc:IssuerParametersUID>urn:patras:studentissuer-uprove</abc:IssuerParametersUID>
    <abc:UnknownAttributes/>
  </abc:CredentialTemplate>
