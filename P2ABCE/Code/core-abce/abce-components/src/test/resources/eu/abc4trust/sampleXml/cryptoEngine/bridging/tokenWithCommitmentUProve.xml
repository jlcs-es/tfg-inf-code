<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- This is a sample ABC4Trust presentation token with commitments produced by UProve. 
This is what we expect the uprove engine to produce as a reply to the presentationtokendescriptionwithcommitments
produced given presentation.xml. -->

<abc:PresentationTokenWithCommitments xmlns:abc="http://abc4trust.eu/wp2/abcschemav1.0"
  Version="Version 1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://abc4trust.eu/wp2/abcschemav1.0 ../../../../../../../../../abc4trust-xml/src/main/resources/xsd/schema.xsd">
  <abc:PresentationTokenDescriptionWithCommitments PolicyUID="mypolicy">
    <abc:Credential Alias="student">
      <abc:CredentialSpecUID>urn:patras:studentcredspec</abc:CredentialSpecUID>
      <abc:IssuerParametersUID>urn:patras:studentissuer-uprove</abc:IssuerParametersUID>
      <abc:DisclosedAttribute AttributeType="urn:patras:study">
      	<abc:AttributeValue>Computer Science</abc:AttributeValue>
      </abc:DisclosedAttribute>
      <abc:CommittedAttribute AttributeType="urn:patras:matriculation">
      	<abc:Commitment/><!-- (Crypto blob)</abc:Commitment> -->
      	<abc:CommittedValue/><!-- (Crypto blob 2)</abc:CommittedValue>-->
      	<abc:OpeningInformation/><!-- (Crypto blob 3)</abc:CommittedInformation>-->
      </abc:CommittedAttribute>	
    </abc:Credential>
  </abc:PresentationTokenDescriptionWithCommitments>
  <abc:CryptoEvidence/><!-- (U-Prove crypto blob)</abc:CryptoEvidence>-->  
</abc:PresentationTokenWithCommitments>