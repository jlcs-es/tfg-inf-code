<?xml version='1.0' encoding='UTF-8' standalone='yes'?>

<!-- A Simple issuance message that is a wrapper around an IssuanceToken -->

<abc:IssuanceMessage xmlns:abc='http://abc4trust.eu/wp2/abcschemav1.0' Context='context2'
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://abc4trust.eu/wp2/abcschemav1.0 ../../../../../../../abc4trust-xml/src/main/resources/xsd/schema.xsd">
  <abc:IssuanceToken Version='1.0'>
    <abc:IssuanceTokenDescription>
      <abc:PresentationTokenDescription PolicyUID='my-policy-uid' />
      <abc:CredentialTemplate>
        <abc:CredentialSpecUID>spec2</abc:CredentialSpecUID>
        <abc:IssuerParametersUID>issuerparams2</abc:IssuerParametersUID>
        <abc:UnknownAttributes />
      </abc:CredentialTemplate>
    </abc:IssuanceTokenDescription>
    <abc:CryptoEvidence />
  </abc:IssuanceToken>
</abc:IssuanceMessage>
